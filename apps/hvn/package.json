{
  "name": "hvn",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "hvn": "cross-env NODE_OPTIONS=--no-deprecation next dev -p 3001",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev -p 3001",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev -p 3001",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start -p 3001"
  },
  "dependencies": {
    "@fc/ui": "workspace:*",
    "@hookform/resolvers": "^4.1.3",
    "@payloadcms/db-mongodb": "^3.25.0",
    "@payloadcms/email-nodemailer": "^3.25.0",
    "@payloadcms/next": "^3.25.0",
    "@payloadcms/payload-cloud": "^3.25.0",
    "@payloadcms/plugin-form-builder": "^3.25.0",
    "@payloadcms/richtext-lexical": "^3.25.0",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@tanstack/react-query": "^5.67.1",
    "axios": "^1.8.1",
    "class-variance-authority": "^0.7.1",
    "cross-env": "^7.0.3",
    "framer-motion": "^12.4.10",
    "graphql": "^16.10.0",
    "lexical": "^0.27.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.477.0",
    "next": "^15.2.1",
    "next-intl": "^3.26.5",
    "next-themes": "^0.4.4",
    "nodemailer": "^6.10.0",
    "payload": "^3.25.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.4.1",
    "react-icons": "^5.5.0",
    "sharp": "0.33.5",
    "sonner": "^2.0.1",
    "stripe": "17.7.0"
  },
  "devDependencies": {
    "@fc/codegen": "workspace:*",
    "@fc/config-eslint": "workspace:*",
    "@fc/config-payload": "workspace:*",
    "@fc/config-tailwind": "workspace:*",
    "@fc/config-typescript": "workspace:*",
    "@fc/intl": "workspace:*",
    "@fc/types": "workspace:*",
    "@fc/ui": "workspace:*",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20.17.23",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3"
  }
}
